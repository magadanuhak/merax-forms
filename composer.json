{
    "name": "merax/forms",
    "description": "Merax Forms Generator Package",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.2",
        "illuminate/support": "~5.6.34|~5.7.0|~5.8.0|^6.0",
        "nesbot/carbon": "^1.26.3 || ^2.0",
        "spatie/laravel-query-builder" : "~2.0",
        "illuminate/database": "~5.6.34|~5.7.0|~5.8.0|^6.0"

    },
    "autoload": {
        "psr-4": {"MeraxForms\\": "src/"}
    },
    "config": {
